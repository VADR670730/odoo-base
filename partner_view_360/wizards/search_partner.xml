<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="search_partner_form" model="ir.ui.view">
        <field name="name">search.partner.wizard</field>
        <field name="model">search.partner</field>
        <field name="arch"> 
            <form string="Search Partner">
                <group>
                    <field name="search_reason"/>
                    <field name="search_string"/>
                </group>
            </form>
        </field>
    </record>
    <record id="search_partner_wizard" model="ir.actions.act_window">
        <field name="name">Search Partner</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">search.partner</field>
        <field name="view_mode">form</field>
        <field name="view_id" ref="search_partner_form"/>
        <field name="target">new</field>
    </record>
</odoo>